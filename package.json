  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "cors": "^2.8.5",
    "d3": "^5.7.0",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.5",
    "nodejieba": "^2.3.0",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-autosize-textarea": "^5.0.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "should": "^13.2.3",
    "uglifyjs-webpack-plugin": "^2.0.1"
  },
  "sideEffects": [
    "./src/math.js",
    "*.css"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "csv-loader": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.85.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.8",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4",
    "winston": "^3.1.0",
    "xml-loader": "^1.2.1"
  },
  "scripts": {
    "start": "npm dev:start",
    "flow": "flow init",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node app",
    "dev:wds": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint src webpack.common.js --ext .js,.jsx",
    "test": "npm lint && flow && jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "npm test",
    "prepush": "npm test && yarn prod:build",
    "watch": "webpack --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "jest": {
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "^react(.*)$": "<rootDir>/vendor/react-master$1",
      "^config$": "<rootDir>/configs/app-config.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__jest__/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarityAcuity/test.git"
  },
  "keywords": [],
  "author": "ClarityAcuity",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClarityAcuity/test/issues"
  },
  "homepage": "https://github.com/ClarityAcuity/test#readme",
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
